<% include ../include/header %>

<script>
    // 대표이미지 크기 1:1 맞추기
    var mainImgW = $('.mainBJimg').width();
    if($(window).width()>=769) {
        var mainImgW = $('.mainBJimg').width();
        $('.mainBJimg').height(mainImgW);
        $('.mainBJinfo').height(mainImgW-70);
    }
    else {
        var mainImgW = $('.mainBJimg').width();
        $('.mainBJimg').height(mainImgW);
    }
    $(window).resize(function() {
        if($(window).width()>=769) {
            var mainImgW = $('.mainBJimg').width();
            $('.mainBJimg').height(mainImgW);
            $('.mainBJinfo').height(mainImgW-70);
        }
        else {
            var mainImgW = $('.mainBJimg').width();
            $('.mainBJimg').height(mainImgW);
        }
    });

    // 썸네일 스크립트
    $('.btm-l .imgbox').click(function() {
       imgIdx = $(this).index();
       var imageUrl = $(this).find('.thumimg').css('background-image');
       $(this).parents('.btm-l').siblings('.btm-r').find('.imgbox>.subimg').css('background-image',imageUrl);
       $('.thumimg').removeClass('showOn');
       $(this).find('.thumimg').addClass('showOn');
    });

    $('.voteBtn>a').click(function(e) {
        e.preventDefault();
    });

    $('.coinList>li').click(function() {
        var coinIdx = $(this).index();
        $('.coinList>li').removeClass('on');
        $(this).addClass('on');
        $('.addr').hide();
        $('.addr').eq(coinIdx).show();
    });

    // 팝업
    popupOn('#btn_share', '.share', '.popbg');
    popupOff('.share .title>img');
    popupOn('.voteBtn', '.useNotice', '.popbg');
    popupOff('.useNotice .title>img');
    popupOff('.useNotice #btn_agree');
    popupOn('#btn_agree', '.coinVote', '.popbg')
    popupOff('.coinVote .title>img');
    popupOff('.coinVote #btn_close');
    popupOn('.btn_qr', '.qrCodePop', '.popbg2');
    popupOff2('.qrCodePop .title>img');
    popupOff2('.qrCodePop #btn_close');
    popupOn('.btn_copy_qr', '.alertPop', '.popbg2');
    popupOff2('.alertPop .title>img');
    popupOff2('.alertPop #btn_close');
    popupOn('.link .shareI', '.alertPop', '.popbg2');
//});
// 팝업띄우기 popupOn('클릭하는 버튼');
function popupOn(classN, popupN, bg) {
    $(classN).click(function() {
        // 스크롤 막기
        $('#popbg').on('scroll touchmove mousewheel', function(e) {
            e.preventDefault();
            e.stopPropagation();
            return false;
        });

        $(this).parents('#wrap').find(bg).stop().fadeIn(100);
        $(this).parents('#wrap').find(popupN).stop().fadeIn(100).css('display','block');
    });
}

// 팝업닫기 popupOff('팝업창의 X버튼'); > popupOff('#popup .title>img');
function popupOff(classN) {
    $(classN + ', .popbg').click(function() {
        // 스크롤 다시 허용
        $('html, body').off('scroll touchmove mousewheel');
        $(this).parents('#wrap').find('.popbg').stop().fadeOut(100);
        $(this).parents('#wrap').find('.popbg').nextAll().stop().fadeOut(100);
    });
}
function popupOff2(classN) {
    $(classN + ', .popbg2').click(function() {
        // 스크롤 다시 허용
        $('html, body').off('scroll touchmove mousewheel');
        $(this).parents('#wrap').find('.popbg2').stop().fadeOut(100);
        $(this).parents('#wrap').find('.popbg2').nextAll().stop().fadeOut(100);
    });
}
</script>

